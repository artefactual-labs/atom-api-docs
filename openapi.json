  "components": {
    "parameters": {
      "Limit": {
        "name": "limit",
        "in": "query",
        "description": "Limit number of results returned",
        "required": false,
        "schema": {
          "type": "integer",
          "default": 10,
          "example": 25
        }
      },
      "SfCulture": {
        "name": "sf_culture",
        "in": "query",
        "description": "ISO language code (e.g. \"en\", \"fr\")",
        "required": false,
        "schema": {
          "type": "string",
          "default": "en",
          "minLength": 2,
          "maxLength": 3
        }
      },
      "Skip": {
        "name": "skip",
        "in": "query",
        "description": "Skip first N results",
        "required": false,
        "schema": {
          "type": "integer",
          "default": 0,
          "example": 10
        }
      },
      "Slug": {
        "name": "slug",
        "in": "path",
        "required": true,
        "description": "A resource slug",
        "schema": {
          "type": "string",
          "example": "woolworth-building-sudbury-ontario"
        }
      }
    },
