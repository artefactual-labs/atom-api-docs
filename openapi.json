  "paths": {
    "/api": {
      "get": {
        "summary": "Get AtoM version",
        "operationId": "version",
        "tags": [
          "application"
        ],
        "responses": {
          "200": {
            "description": "The AtoM version number",
            "content": {
              "application/json": {
                "schema": {
                  "required": [
                    "version"
                  ],
                  "properties": {
                    "version": {
                      "type": "string"
                    }
                  },
                  "example": {
                    "version": "2.6.0"
                  }
                }
              }
            }
          },
          "401": {
            "description": "Not authorized error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/401NotAuthorizedError"
                }
              }
            }
          },
          "500": {
            "description": "Internal error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/500InternalError"
                }
              }
            }
          }
        }
      }
    },
    "/api/informationobjects": {
      "get": {
        "summary": "A paged array of information objects with summary data",
        "operationId": "informationobjectsBrowse",
        "tags": [
          "informationobjects"
        ],
        "parameters": [
          {
            "$ref": "#/components/parameters/SfCulture"
          },
          {
            "$ref": "#/components/parameters/Limit"
          },
          {
            "$ref": "#/components/parameters/Skip"
          }
        ],
        "responses": {
          "200": {
            "description": "A paged array of information objects with summary data",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/InformationObjectList"
                }
              }
            }
          },
          "401": {
            "description": "Not authorized error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/401NotAuthorizedError"
                }
              }
            }
          },
          "500": {
            "description": "Internal error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/500InternalError"
                }
              }
            }
          }
        }
      },
      "post": {
        "summary": "Create a new information object",
        "operationId": "informationbjectsCreate",
        "tags": [
          "informationobjects"
        ],
        "parameters": [
          {
            "$ref": "#/components/parameters/SfCulture"
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/InformationObjectCreateBody"
              }
            }
          }
        },
        "responses": {
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "properties": {
                    "id": {
                      "type": "integer"
                    },
                    "slug": {
                      "type": "string"
                    },
                    "parent_id": {
                      "type": "integer"
                    }
                  },
                  "example": {
                    "id": 2004327,
                    "slug": "woolworth-building-sudbury-ontario",
                    "parent_id": 71473
                  }
                }
              }
            }
          },
          "401": {
            "description": "Not authorized error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/401NotAuthorizedError"
                }
              }
            }
          },
          "500": {
            "description": "Internal error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/500InternalError"
                }
              }
            }
          }
        }
      }
    },
